public class GCD {
    public static int gcd(int x, int y) {
        if (y == 0) {
            return x;
        }
        return gcd(y, x % y);
    }

    public static void main(String[] args) {
        int x = 56; 
        int y = 98;
        System.out.println("GCD of " + x + " and " + y + " is: " + gcd(x, y));
    }
}
